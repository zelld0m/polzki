@model Exam.Models.SingleAttendee;
<br />
<br />
<br />


<div class="row">
    <div class="col-lg-12">
        <form method="get" asp-controller="Home" asp-action="AddAttendee">
            <label asp-for="Data.Name">Name:</label>
            <input type="text" id="Name" asp-for="Data.Name" name="AttendeeName" />
            <label>EMAIL</label>
            <input type="text" id="Name" asp-for="Data.Email" name="Email" />
            <label>Event ID</label>
            <input type="text" id="EventID" asp-for="Data.Event" name="EventID" />
            <input type="submit" value="Submit" />

        </form>
        <div>
    <h4>Data</h4>
    <hr />
    <dl class="row">
       <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Message)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Data)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Data)
        </dd>
    </dl>
</div>









