@model Exam.Models.EventsResponse;
@{
    ViewData["Title"] = "Event";
}
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
<div class="row">
    <div class="col-lg-12">
        <table><br>
            <tr>
                <td>Id</td>
                
                <td>Event Name</td>
            </tr>

            @foreach (var item in Model.Data)
            {
                <tr>
                    <td>@item.ID</td>
                    <td>@item.EventName</td>
                </tr>
            }

            
            </table>
    </div>
</div>
